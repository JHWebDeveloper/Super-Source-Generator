{
	"name": "super-source-generator",
	"version": "1.0.4",
	"description": "Super Source Generator is a web app developed by Jonathan Hamilton to help Ch9 employees produce single or multiple source overlays on the fly. No photoshop experience necessary.",
	"main": "build/main.js",
	"scripts": {
		"lint": "eslint . --ext .js",
		"dev": "webpack serve --config webpack.dev.js",
		"build": "rimraf build && webpack --config webpack.prod.js",
		"pack": "rimraf dist && electron-builder --dir",
		"dist:mac": "rimraf dist && npm run build && electron-builder -m",
		"dist:win": "rimraf dist && npm run build && electron-builder -w",
		"deploy:mac": "rimraf dist && npm run build && electron-builder build -m --publish always",
		"deploy:win": "rimraf dist && npm run build && electron-builder build -w --publish always",
		"postinstall": "electron-builder install-app-deps",
		"start": "electron ."
	},
	"author": "Jonathan Hamilton",
	"license": "CC-BY-NC-4.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/JHWebDeveloper/Super-Source-Generator.git"
	},
	"dependencies": {
		"electron-log": "^4.3.1",
		"electron-updater": "^4.3.5",
		"prop-types": "^15.7.2",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"to-css": "^1.2.1",
		"uuid": "^8.3.2"
	},
	"devDependencies": {
		"@babel/core": "^7.12.10",
		"@babel/preset-env": "^7.12.11",
		"@babel/preset-react": "^7.12.10",
		"@babel/register": "^7.12.10",
		"babel-loader": "^8.2.2",
		"copy-webpack-plugin": "^8.1.1",
		"core-js": "^3.8.3",
		"css-loader": "^5.0.1",
		"css-minimizer-webpack-plugin": "^2.0.0",
		"electron": "^11.2.1",
		"electron-builder": "^22.9.1",
		"eslint": "^7.18.0",
		"eslint-plugin-react": "^7.22.0",
		"html-webpack-plugin": "^5.3.1",
		"mini-css-extract-plugin": "^1.3.4",
		"postcss": "^8.2.4",
		"postcss-loader": "^5.2.0",
		"postcss-preset-env": "^5.3.0",
		"postcss-selector-matches": "^3.0.1",
		"regenerator-runtime": "^0.13.7",
		"rimraf": "^3.0.2",
		"webpack": "^5.18.0",
		"webpack-cli": "^4.4.0",
		"webpack-dev-server": "^3.11.2",
		"webpack-merge": "^5.7.3",
		"webpack-node-externals": "^3.0.0"
	},
	"browserslist": "last 2 electron versions"
}
