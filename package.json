{
  "name": "super-source-generator",
  "version": "1.0.1",
  "description": "Super Source Generator is a web app developed by Jonathan Hamilton to help Ch9 employees produce single or multiple source overlays on the fly. No photoshop experience necessary.",
  "main": "build/main.js",
  "scripts": {
    "lint": "eslint . --ext .js",
    "dev": "webpack-dev-server --config webpack.dev.js",
    "build": "rimraf build && webpack --config webpack.prod.js",
    "pack": "rimraf dist && electron-builder --dir",
    "dist": "rimraf dist && npm run build && electron-builder -w -m",
    "deploy": "rimraf dist && npm run build && electron-builder build -w -m --publish always",
    "postinstall": "electron-builder install-app-deps",
    "start": "electron ."
  },
  "author": "Jonathan Hamilton",
  "license": "CC-BY-NC-4.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/JHWebDeveloper/Super-Source-Generator.git"
  },
  "dependencies": {
    "electron-log": "^4.2.4",
    "electron-updater": "^4.3.5",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "to-css": "^1.2.1",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.11.5",
    "babel-loader": "^8.1.0",
    "babel-preset-minify": "^0.5.1",
    "copy-webpack-plugin": "^5.1.2",
    "core-js": "^3.6.5",
    "css-loader": "^3.6.0",
    "cssnano": "^4.1.10",
    "electron": "^7.3.3",
    "electron-builder": "^22.8.1",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.21.0",
    "file-loader": "^4.3.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.2",
    "nodemon": "^2.0.4",
    "postcss": "^7.0.34",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "url-loader": "^3.0.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-node-externals": "^1.7.2"
  },
  "browserslist": "electron 6.0.1"
}
